{
  "name": "@wunderwerk/ts-results",
  "version": "0.1.0",
  "description": "A typescript implementation of Rust's Result and Option objects.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "pnpm run clean && tsup src/index.ts --dts --format esm,cjs --out-dir ./dist && tsup src/rxjs-operators/index.ts --external rxjs --dts --format esm,cjs --out-dir ./dist/rxjs-operators",
    "clean": "rm -rf dist",
    "test": "jest",
    "format": "prettier . --write",
    "typecheck": "tsc --project ./tsconfig.json --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wunderwerkio/ts-results.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/jest": "^29.5.0",
    "conditional-type-checks": "^1.0.6",
    "copyfiles": "^2.4.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "rxjs": "^7.8.0",
    "ts-jest": "^29.0.5",
    "tsup": "^6.6.3",
    "typescript": "^5.0.2"
  },
  "optionalDependencies": {
    "rxjs": "^7.8.0"
  }
}
